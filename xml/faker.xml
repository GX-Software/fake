<faker>
	<class>
		<title>电动压力变送器</title>
		<type>ptrans</type>
		<defsel>2</defsel>
		<!--precision：精度等级；accepte：允许误差；hystere：回程误差；std：标准表误差分度；pt：检定点最小数量-->
		<item><precision>0.05</precision><accepte>0.05</accepte><hystere>0.05</hystere><std>3</std><pt>9</pt></item>
		<item><precision>0.1</precision><accepte>0.1</accepte><hystere>0.08</hystere><std>3</std><pt>9</pt></item>
		<item><precision>0.2</precision><accepte>0.2</accepte><hystere>0.16</hystere><std>4</std><pt>5</pt></item>
		<item><precision>0.25</precision><accepte>0.25</accepte><hystere>0.2</hystere><std>4</std><pt>5</pt></item>
		<item><precision>0.5</precision><accepte>0.5</accepte><hystere>0.4</hystere><std>4</std><pt>5</pt></item>
		<item><precision>1</precision><accepte>1</accepte><hystere>0.8</hystere><std>4</std><pt>5</pt></item>
		<item><precision>1.5</precision><accepte>1.5</accepte><hystere>1.2</hystere><std>4</std><pt>5</pt></item>
		<item><precision>2</precision><accepte>2</accepte><hystere>1.6</hystere><std>4</std><pt>5</pt></item>
	</class>
	<class>
		<title>压力控制器（压力开关）</title>
		<type>pctrl</type>
		<defsel>2</defsel>
		<!--precision：精度等级；accepte：允许误差；repeate：重复性误差；std：标准表误差分度；-->
		<item><precision>0.5</precision><accepte>0.5</accepte><repeate>0.5</repeate><std>4</std></item>
		<item><precision>1</precision><accepte>1</accepte><repeate>1</repeate><std>4</std></item>
		<item><precision>1.5</precision><accepte>1.5</accepte><repeate>1.5</repeate><std>4</std></item>
		<item><precision>2</precision><accepte>2</accepte><repeate>2</repeate><std>4</std></item>
		<item><precision>2.5</precision><accepte>2.5</accepte><repeate>2.5</repeate><std>4</std></item>
		<item><precision>4</precision><accepte>4</accepte><repeate>4</repeate><std>4</std></item>
	</class>
	<class>
		<title>弹性元件式一般压力表</title>
		<type>pscale</type>
		<defsel>1</defsel>
		<!--precision：精度等级；accepte：允许误差；maxe：达到90%以上允许误差；std：标准表误差分度；pt：检定点最小数量-->
		<item><precision>1</precision><accepte>1</accepte><maxe>1.6</maxe><std>4</std><pt>5</pt></item>
		<item><precision>1.6</precision><accepte>1.6</accepte><maxe>2.5</maxe><std>4</std><pt>5</pt></item>
		<item><precision>1.5</precision><accepte>1.6</accepte><maxe>2.5</maxe><std>4</std><pt>5</pt></item>
		<item><precision>2.5</precision><accepte>2.5</accepte><maxe>4</maxe><std>4</std><pt>5</pt></item>
		<item><precision>4</precision><accepte>4</accepte><maxe>4</maxe><std>4</std><pt>5</pt></item>
	</class>
</faker>